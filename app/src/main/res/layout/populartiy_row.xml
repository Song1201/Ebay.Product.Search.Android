<?xml version="1.0" encoding="utf-8"?>
<TableRow xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:layout_marginBottom="16dp">
	<TextView
		android:id="@+id/detailShippingRowHeader"
		android:layout_weight="2"
		android:layout_width="0dp"
		android:textColor="@android:color/black"
		android:layout_marginLeft="40dp"
		android:textSize="16dp"
		android:text="Popularity"/>
	<com.wssholmes.stark.circular_score.CircularScoreView
		android:id="@+id/detailShippingRowCell"
		android:layout_weight="3"
		android:layout_width="28dp"
		android:layout_height="28dp"
		android:textSize="18sp"
		android:layout_marginStart="24dp"
		app:setPrimaryColor="@color/btnOrange"
		app:setSecondaryColor="@color/grayText"
		app:setBackgroundColor="@android:color/white"
		app:setTextColor="@android:color/black"/>

</TableRow>